<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="scoproject.com.peoplemvvm">

    <application
        android:name=".PeopleMVVM"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:parentActivityName=".view.home.HomeActivity"
            android:name=".view.addmember.AddMemberActivity"
            android:label="Add Member"/>
        <activity
            android:parentActivityName=".view.listpeople.ListPeopleActivity"
            android:name=".view.listpeople.ListPeopleActivity"
            android:label="People List"/>
    </application>

</manifest>